{
  "name": "dpk-ui",
  "version": "0.0.3",
  "description": "Web Components UI Library built with Stencil.js",
  "keywords": [
    "web-components",
    "ui-library",
    "stencil",
    "modal",
    "typescript",
    "custom-elements"
  ],
  "author": {
    "name": "G.Olshevskiy",
    "email": "graf.olshevskiy@gmail.com"
  },
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/dpk-ui/dpk-ui.esm.js",
  "repository": {
    "type": "",
    "url": ""
  },
  "tag": "latest",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "files": [
    "dist/",
    "loader/"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    },
    "./loader": {
      "types": "./loader/index.d.ts",
      "import": "./loader/index.mjs",
      "require": "./loader/index.cjs"
    }
  },
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^4.26.0",
    "@stencil/sass": "^3.0.12",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.126",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
